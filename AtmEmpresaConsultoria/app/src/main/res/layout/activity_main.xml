<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="atmempresaconsultoria.cursoandroid.com.atmempresaconsultoria.MainActivity">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:background="?attr/colorBackgroundFloating"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="47dp"
            android:layout_marginStart="47dp"
            android:layout_marginTop="27dp"
            app:srcCompat="@drawable/logo"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/empresaId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/imageView"
            android:layout_alignStart="@+id/imageView"
            android:layout_below="@+id/imageView"
            android:layout_marginTop="36dp"
            app:srcCompat="@drawable/menu_empresa"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/servicoId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView"
            android:layout_alignRight="@+id/imageView"
            android:layout_alignTop="@+id/empresaId"
            android:adjustViewBounds="false"
            app:srcCompat="@drawable/menu_servico"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/clienteId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/empresaId"
            android:layout_alignStart="@+id/empresaId"
            android:layout_below="@+id/empresaId"
            android:layout_marginTop="45dp"
            app:srcCompat="@drawable/menu_cliente"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/contatoId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/servicoId"
            android:layout_alignStart="@+id/servicoId"
            android:layout_alignTop="@+id/clienteId"
            app:srcCompat="@drawable/menu_contato"
            tools:ignore="ContentDescription" />
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
